   <string notr="true">QWidget {
    background-color: #ffffff;
    font-family: &quot;Microsoft YaHei&quot;;
}

QLabel {
    color: #333333;
    font-size: 14px;
}

QLineEdit {
    padding: 5px 10px;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
    background-color: #ffffff;
    font-size: 13px;
}

QLineEdit:focus {
    border: 1px solid #4a9eff;
}

QPushButton {
    background-color: #4a9eff;
    color: white;
    border: none;
    border-radius: 4px;
    padding: 5px 15px;
    font-size: 13px;
}

QPushButton:hover {
    background-color: #3d8ee6;
}

QPushButton:pressed {
    background-color: #2d7ed6;
}

QPushButton:disabled {
    background-color: #cccccc;
    color: #666666;
}

/* 右侧功能按钮组的特殊样式 */
#pushButton, #pushButton_2, #pushButton_6, #pushButton_9, #pushButton_10 {
    background-color: #67c23a;
    font-size: 14px;
    font-weight: bold;
}

#pushButton:hover, #pushButton_2:hover, #pushButton_6:hover, #pushButton_9:hover, #pushButton_10:hover {
    background-color: #5daf34;
}

#pushButton:pressed, #pushButton_2:pressed, #pushButton_6:pressed, #pushButton_9:pressed, #pushButton_10:pressed {
    background-color: #529d2e;
}

QComboBox {
    padding: 5px 10px;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
    background-color: #ffffff;
    font-size: 13px;
}

QComboBox:hover {
    border: 1px solid #4a9eff;
}

QTableWidget {
    border: 1px solid #d0d0d0;
    background-color: #ffffff;
    gridline-color: #e0e0e0;
}

QTableWidget::item {
    padding: 5px;
}

QTableWidget::item:selected {
    background-color: #e6f3ff;
    color: #333333;
}

QHeaderView::section {
    background-color: #f5f5f5;
    padding: 5px;
    border: none;
    border-right: 1px solid #d0d0d0;
    border-bottom: 1px solid #d0d0d0;
    font-weight: bold;
    color: #333333;
}

QRadioButton {
    color: #333333;
    font-size: 14px;
}

QRadioButton::indicator {
    width: 18px;
    height: 18px;
}

QRadioButton::indicator:unchecked {
    border: 2px solid #d0d0d0;
    border-radius: 9px;
    background-color: #ffffff;
}

QRadioButton::indicator:checked {
    border: 2px solid #4a9eff;
    border-radius: 9px;
    background-color: #4a9eff;
}

/* 操作按钮组的特殊样式 */
